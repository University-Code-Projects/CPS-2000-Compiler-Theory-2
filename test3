

var Z : int = 1;
set Z = 9.9+1;


var n : real = 1;
var y : real = 1;
var x : real = 1;
if( n>=0 )
    {
        while(n>0)
        {
            set y = y * x;  //Assignment y = y *x;
            set n = n -1;   //Assignment n = n-1;
        }
    }
    else
    {
        while(n<0)
        {
            set y = y / x;    //Assignment y = y / x;
            set n = n + 1;    //Assignment n = n + 1;
        }
    }


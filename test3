
var a : int = 5*5;
var bef : string = "Before";

var af : string = "After";

write a;
write bef;


write "before write";

write "after write";

write bef;

def funcSquare(x:real) : real{
    write "value of x is ";
    write x;
    return x*x;
}

write "Before function call";

write funcSquare(2.0);

write "After func Call";

